@model IEnumerable<AlphaScheduler.Models.Sede>

@{
    ViewBag.Title = "Sede";
}
@section scripts{

    <script src="~/Scripts/Proyecto/Sede.js"></script>

}
<h2>Sede</h2>

<div id="form">
    <form id="formContainer">
        <div>
            <span>Nombre de la sede</span>
            <input type="text" id="Nombre" name="Nombre" />
        </div>
        <div>
            <span>Ciudad de la sede</span>
            <input type="text" id="Ubicacion" name="Ubicacion" />
        </div>
        <div>
            <a id="btnEnviar" class="link" href="#"><span>Enviar</span></a>
        </div>
    </form>
</div>
  @if(@Model.FirstOrDefault() != null){
<div id="Details">
    <h4>DETALLES</h4>
    <table cellspacing="0" cellpadding="0" border="0" class="table table-striped table-bordered" id="tableDetails">
        <thead>
            <tr>

                <td><strong>Nombre Sede</strong></td>
                <td><strong>Ciudad</strong></td>

            </tr>

        </thead>
        @foreach (var i in Model.ToList())
        {
            <tr>

                <td>
                    @i.Nombre
                </td>
                <td>
                    @i.Ubicacion
                </td>

            </tr>
        }
    </table>
</div>
}else{
        <h3>No existen datos que Mostrar </h3>
}

